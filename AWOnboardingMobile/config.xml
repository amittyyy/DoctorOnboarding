    </platform>
    <allow-navigation href="*" />
    <allow-navigation href="http://localhost:8080/*" />
    <allow-navigation href="http://10.0.1.79:8100" />
    <allow-navigation href="http://10.0.1.79:8101" />
    <allow-navigation href="http://10.0.1.79:8102" />
    <allow-navigation href="http://10.0.1.79:8103" />
    <allow-navigation href="http://10.0.1.79:8104" />
    <allow-navigation href="http://10.0.1.79:8105" />
    <plugin name="cordova-plugin-actionsheet" spec="^2.3.3" />
    <plugin name="cordova-plugin-file" spec="^6.0.1" />
    <plugin name="cordova-plugin-camera" spec="^4.0.3" />
    <plugin name="cordova-plugin-file-opener2" spec="^2.0.19" />
    <plugin name="cordova-plugin-inappbrowser" spec="^3.0.0" />
    <plugin name="cordova-sqlite-storage" spec="^2.3.3" />
    <plugin name="cordova-plugin-splashscreen" spec="^5.0.2" />
    <plugin name="cordova-plugin-ionic-webview" spec="^2.0.2" />
    <plugin name="cordova-plugin-ionic-keyboard" spec="^2.1.2" />
    <plugin name="cordova-plugin-chooser" spec="^1.2.1" />
    <plugin name="com-sarriaroman-photoviewer" spec="^1.1.18" />
    <plugin name="cordova-plugin-fingerprint-aio" spec="^1.3.8">
        <variable name="FACEID_USAGE_DESCRIPTION" value=" " />
    </plugin>
    <plugin name="cordova-plugin-android-permissions" spec="^1.0.0" />
    <plugin name="cordova-plugin-filechooser" spec="^1.0.1" />
    <plugin name="cordova-plugin-filepath" spec="^1.4.2" />
    <plugin name="cordova-plugin-whitelist" spec="^1.3.3" />
    <plugin name="cordova-android-support-gradle-release" spec="^1.4.4">
        <variable name="ANDROID_SUPPORT_VERSION" value="27.+" />
    </plugin>
    <plugin name="cordova-plugin-device" spec="^2.0.2" />
    <plugin name="cordova-plugin-fcm-with-dependecy-updated" spec="^2.2.5" />
    <engine name="android" spec="7.1.4" />
    <engine name="browser" spec="5.0.4" />
</widget>
